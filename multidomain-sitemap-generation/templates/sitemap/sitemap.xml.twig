<?xml version="1.0" encoding="UTF-8"?>
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for section in sections %}
        <sitemap>
            <loc>{{ section }}</loc>
            <lastmod>{{ 'now'|date('Y-m-d') }}</lastmod>
        </sitemap>
    {% endfor %}
</sitemapindex>
